code
class Solution{
public:	
	
	
	int isPlaindrome(string s)
	{
	    // Your code goes here
	    int i=0;
	    int j=s.length()-1;
	    while(i<j)
	    {
	        if(s[i]!=s[j])
	            return 0;
	        i++;
	        j--;
	    }
	    return 1;
	}

};
