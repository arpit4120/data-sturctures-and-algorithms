code
#include<bits/stdc++.h>
using namespace std;
 int main()
 {
 	string a,b;
 	getline(cin,a);
	getline(cin,b);
	string c=a+a;
	if(c.find(b)==true)
	{
		cout<<"yes";
		return 0;
	}
	cout<<"no";
	return 0;
 }
