code
#include <bits/stdc++.h>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    string g;
	    cin>>g;
	    for(int i=0;i<g.length();i++)
	    {
	        cout<<g[i];
	        while(i+1<g.length()&&g[i+1]==g[i])
	            i++;
	    }
	    cout<<endl;
	    
	}
	return 0;
}
